{
  "extends": "../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "./node_modules/cypress",
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "./cypress.config.ts"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "next",
      "node",
      "react",
      "react-dom",
      "jest"
    ],
    "jsx": "preserve"
  }
}
